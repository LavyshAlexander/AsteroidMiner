<UserControl x:Class="Game.Newt.Testers.Newt2Tester.FluidEmulation"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Orientation="Vertical">
        <CheckBox Name="chkFluid" Content="Fluid" Checked="chkFluid_Checked" Unchecked="chkFluid_Checked"/>

        <Label Content="Viscosity"/>
        <Slider Name="trkViscosity" Minimum="0" Maximum="3" Value="1" ValueChanged="trkViscosity_ValueChanged"/>
    </StackPanel>
</UserControl>
